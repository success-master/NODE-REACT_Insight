{
  "name": "express-mysql-rest",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "dev": "nodemon --watch .env --watch app.js --watch bin ./bin/www --watch controller --watch db --watch routes --watch core --watch config",
    "db": "sequelize init",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "test": "mocha"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^5.0.1",
    "express": "~4.16.0",
    "google-spreadsheet": "^3.0.11",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "javascript-time-ago": "^2.0.7",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "morgan": "~1.9.0",
    "mysql2": "^1.5.3",
    "sequelize": "^4.37.8",
    "sequelize-paginate": "^1.1.6"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5"
